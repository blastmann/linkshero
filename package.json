{
  "name": "links-hero",
  "private": true,
  "version": "0.1.1",
  "description": "Chrome 扩展：扫描当前页下载链接并推送到 aria2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:all": "vitest run && playwright test",
    "zip": "cd dist && bestzip ../links-hero-v0.1.1.zip *",
    "release": "npm run build && npm run zip"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/chrome": "^0.0.264",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "bestzip": "^2.2.1",
    "jsdom": "^26.0.0",
    "playwright": "^1.57.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^2.1.9"
  }
}
